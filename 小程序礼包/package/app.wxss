/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/*page{height: 100%;}*/
.search_box{position: fixed;top: 0;left: 0;width: 100%;padding: 20rpx;background-color: #fff;box-sizing: border-box;
}
.search_ipt{padding-left: 82rpx;border:1px solid #f55;display:flex;height: 80rpx;box-sizing: border-box;
background: url(data:image/png;base64,) 20rpx center no-repeat;background-size: auto 60%;

}
.search_ipt input{height: 100%;font-size: 30rpx;flex: 1;}
/*.search_ipt .search_btn{width: 80rpx;background: #fff url() height: 100%;position: static}*/
.tip_list{position: absolute;left: 0;top: 80rpx;right: 0;background-color: #fff;}
.tip_list .list_box{height: 80rpx;overflow: hidden;display: flex;padding:12rpx 20rpx;box-sizing: border-box;border: 1px solid #d2d2d2;border-top:0;border-bottom-style: dashed;}
.tip_list .list_box image{width: 56rpx;height: 56rpx;margin-right: 20rpx;border-radius: 8rpx;}
.tip_list .list_box text{flex: 1;font-size: 24rpx;line-height: 56rpx;}
.package_ctn{padding: 100rpx 20rpx 0;}

.package_list_view{display: flex;align-items:center;height: 160rpx;border-bottom:1px solid #dcdcdc}
.package_list_view >image{width: 100rpx;height: 100rpx;margin-right: 20rpx;}
.package_list_view .suling , .package_list_view .ketao,.package_list_view .yuyue{width: 120rpx;font-size: 24rpx;color: #fff;text-align: center;line-height: 1;border-radius: 6rpx;padding: 13rpx 0 9rpx; }
.package_list_view .suling{background-color: #f55}
.package_list_view .ketao{background-color: #ffc039}
.package_list_view .yuyue{background-color: #55a6ff}
.package_list_view .list_center{font-size: 0;width: 470rpx;}
.package_list_view .list_center text{font-size: 24rpx;line-height: 1.2}
.gray{color: #aaa}
.package_list_view .list_center .pack_name{font-size: 30rpx;line-height:1.5;
  overflow: hidden;white-space: nowrap;text-overflow: ellipsis;display: block
}
.package_list_view .ui_device{display: flex;height: 40rpx;align-items: center;}
.ios,.adr{width: 24rpx;height: 24rpx;margin-right: 10rpx}
/*.ios{background: #fff url(data:image/png;base64,) center center no-repeat;background-size: 100% 100%;}
.adr{background: #fff url(data:image/png;base64,) center center no-repeat;background-size: 100% 100%;}*/
.package_list_view .ui_device text{line-height: 1;display: block;}

.loadingTxt{font-size: 30rpx;text-align: center;display: block;line-height: 80rpx;color: #aaa;}


scroll-view{height: 100%;}
